{
  "name": "ec.components",
  "version": "0.13.0",
  "homepage": "https://github.com/entrecode/ec.components",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl=true --ssl-cert=ssl/server.crt --ssl-key=ssl/server.key --port 1337",
    "build": "ng build  --aot=false --build-optimizer=false --prod --statsJson",
    "analyze": "npx webpack-bundle-analyzer@latest ./dist/ec-components/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerelease": "sh ./scripts/prerelease.sh",
    "postrelease": "sh ./scripts/postrelease.sh",
    "release": "sh ./scripts/release.sh",
    "ssl": "sh ./scripts/ssl.sh",
    "build-packages": "sh ./scripts/build-packages.sh",
    "test-packages": "sh ./scripts/test-packages.sh",
    "changelogs": "sh ./scripts/changelogs.sh",
    "dev-setup": "npm run build-packages && yarn",
    "docs": "compodoc packages -d docs -p tsconfig.json --includes tutorial --includesName Documentation",
    "docs:watch": "compodoc packages -d docs -p tsconfig.json -s -o -w --includes tutorial --includesName Documentation",
    "watch:ui": "ng build ui --watch",
    "watch:data": "ng build data --watch",
    "watch:core": "ng build core --watch",
    "watch:calendar": "ng build calendar --watch"
  },
  "private": true,
  "workspaces": [
    "packages/*/dist"
  ],
  "dependencies": {
    "@agm/core": "1.0.0-beta.5",
    "@angular/animations": "^7.2.7",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/forms": "^7.2.7",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "@ec.components/ace": "*",
    "@ec.components/calendar": "*",
    "@ec.components/core": "*",
    "@ec.components/data": "*",
    "@ec.components/location": "*",
    "@ec.components/medium-editor": "*",
    "@ec.components/style": "*",
    "@ec.components/tinymce": "*",
    "@ec.components/ui": "^0.39.0",
    "ace-builds": "^1.4.3",
    "core-js": "^2.6.5",
    "ec.sdk": "^0.18.2",
    "googlemaps": "^1.12.0",
    "lerna": "^3.13.1",
    "medium-editor": "^5.23.2",
    "moment-es6": "^1.0.0",
    "ngx-cookie": "^2.0.1",
    "ngx-drag-drop": "^1.1.0",
    "rxjs": "~6.3.3",
    "tinymce": "^4.9.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.4",
    "@angular-devkit/build-ng-packagr": "^0.13.4",
    "@angular/cli": "^7.3.4",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "^7.2.7",
    "@compodoc/compodoc": "^1.1.8",
    "@types/jasmine": "^3.3.9",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "conventional-changelog": "^3.0.6",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.7.1",
    "protractor": "~5.4.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "<3.3.0"
  },
  "browser": {
    "vm": false,
    "http": false,
    "https": false,
    "process": false
  }
}
