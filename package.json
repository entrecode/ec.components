{
  "name": "ec.components",
  "version": "0.13.0",
  "homepage": "https://github.com/entrecode/ec.components",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --ssl --port 1337",
    "build": "ng build --aot=false --build-optimizer=false --prod --stats-json",
    "analyze": "npx webpack-bundle-analyzer@latest ./dist/ec-components/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerelease": "sh ./scripts/prerelease.sh",
    "postrelease": "sh ./scripts/postrelease.sh",
    "release": "sh ./scripts/release.sh",
    "build-packages": "sh ./scripts/build-packages.sh",
    "test-packages": "sh ./scripts/test-packages.sh",
    "changelogs": "sh ./scripts/changelogs.sh",
    "dev-setup": "npm run build-packages && yarn",
    "docs": "compodoc packages -d docs -p tsconfig.json --includes tutorial --includesName Documentation",
    "docs:watch": "compodoc packages -d docs -p tsconfig.json -s -o -w --includes tutorial --includesName Documentation",
    "watch:ui": "ng build ui --watch",
    "watch:data": "ng build data --watch",
    "watch:core": "ng build core --watch",
    "watch:calendar": "ng build calendar --watch",
    "watch:medium": "ng build medium-editor --watch",
    "watch:location": "ng build location --watch",
    "postinstall": "cd website && npm i",
    "build:website": "cd website && npm run build",
    "docusaurus": "cd website && npm run start",
    "test:screenshots": "cd scripts && jest screenshot.test.js",
    "test:screenshots-update": "cd scripts && jest screenshot.test.js -u"
  },
  "private": true,
  "workspaces": [
    "packages/*/dist"
  ],
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "^11.2.9",
    "@angular/cli": "^11.2.8",
    "@angular/common": "^11.2.9",
    "@angular/compiler": "^11.2.9",
    "@angular/core": "^11.2.9",
    "@angular/forms": "^11.2.9",
    "@angular/platform-browser": "^11.2.9",
    "@angular/platform-browser-dynamic": "^11.2.9",
    "@angular/router": "^11.2.9",
    "@ec.components/ace": "*",
    "@ec.components/calendar": "*",
    "@ec.components/core": "*",
    "@ec.components/data": "*",
    "@ec.components/location": "*",
    "@ec.components/medium-editor": "*",
    "@ec.components/style": "*",
    "@ec.components/tinymce": "*",
    "@ec.components/ui": "*",
    "ace-builds": "^1.4.12",
    "core-js": "^3.10.1",
    "ec.sdk": "^0.22.9",
    "googlemaps": "^1.12.0",
    "lerna": "^4.0.0",
    "medium-editor": "^5.23.3",
    "moment-es6": "^1.0.0",
    "ngx-cookie": "^5.0.2",
    "ngx-drag-drop": "^2.0.0",
    "rxjs": "~6.6.7",
    "tinymce": "^4.9.11",
    "tslib": "^2.2.0",
    "x.ui": "^10.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.8",
    "@angular/compiler-cli": "^11.2.9",
    "@angular/language-service": "^11.2.9",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.6.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.37",
    "codelyzer": "^6.0.1",
    "conventional-changelog": "^3.1.24",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~6.0.0",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.4.1",
    "karma": "~5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "puppeteer": "^8.0.0",
    "tslint": "~6.1.3",
    "typescript": "4.1.5"
  },
  "browser": {
    "vm": false,
    "http": false,
    "https": false,
    "process": false,
    "global": false
  }
}
