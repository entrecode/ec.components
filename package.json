{
  "name": "ec.components",
  "version": "0.13.0",
  "homepage": "https://github.com/entrecode/ec.components",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl=true --ssl-cert=ssl/server.crt --ssl-key=ssl/server.key --port 1337",
    "build": "ng build  --aot=false --build-optimizer=false --prod --statsJson",
    "analyze": "npx webpack-bundle-analyzer@latest ./dist/ec-components/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerelease": "sh ./scripts/prerelease.sh",
    "postrelease": "sh ./scripts/postrelease.sh",
    "release": "sh ./scripts/release.sh",
    "ssl": "sh ./scripts/ssl.sh",
    "build-packages": "sh ./scripts/build-packages.sh",
    "test-packages": "sh ./scripts/test-packages.sh",
    "changelogs": "sh ./scripts/changelogs.sh",
    "dev-setup": "npm run build-packages && yarn",
    "docs": "compodoc packages -d docs -p tsconfig.json --includes tutorial --includesName Documentation",
    "docs:watch": "compodoc packages -d docs -p tsconfig.json -s -o -w --includes tutorial --includesName Documentation",
    "watch:ui": "ng build ui --watch",
    "watch:data": "ng build data --watch",
    "watch:core": "ng build core --watch"
  },
  "private": true,
  "workspaces": [
    "packages/*/dist"
  ],
  "dependencies": {
    "@agm/core": "1.0.0-beta.3",
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ec.components/ace": "latest",
    "@ec.components/calendar": "latest",
    "@ec.components/core": "latest",
    "@ec.components/data": "latest",
    "@ec.components/location": "latest",
    "@ec.components/medium-editor": "latest",
    "@ec.components/style": "latest",
    "@ec.components/tinymce": "latest",
    "@ec.components/ui": "latest",
    "ace-builds": "^1.4.2",
    "core-js": "^2.5.4",
    "ec.sdk": "^0.18.0",
    "googlemaps": "^1.12.0",
    "lerna": "^3.10.5",
    "medium-editor": "^5.23.2",
    "moment-es6": "^1.0.0",
    "ngx-cookie": "^2.0.1",
    "ngx-drag-drop": "^1.1.0",
    "rxjs": "~6.3.3",
    "tinymce": "^4.9.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@compodoc/compodoc": "^1.1.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "conventional-changelog": "^3.0.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.7.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "browser": {
    "vm": false,
    "http": false,
    "https": false,
    "process": false
  }
}
