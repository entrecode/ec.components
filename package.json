{
  "name": "ec.components",
  "version": "0.13.0",
  "homepage": "https://github.com/entrecode/ec.components",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl=true --ssl-cert=ssl/server.crt --ssl-key=ssl/server.key --port 1337",
    "build": "ng build  --aot=false --build-optimizer=false --prod --statsJson",
    "analyze": "npx webpack-bundle-analyzer@latest ./dist/ec-components/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerelease": "sh ./scripts/prerelease.sh",
    "postrelease": "sh ./scripts/postrelease.sh",
    "release": "sh ./scripts/release.sh",
    "ssl": "sh ./scripts/ssl.sh",
    "build-packages": "sh ./scripts/build-packages.sh",
    "test-packages": "sh ./scripts/test-packages.sh",
    "changelogs": "sh ./scripts/changelogs.sh",
    "dev-setup": "npm run build-packages && yarn",
    "docs": "compodoc packages -d docs -p tsconfig.json --includes tutorial --includesName Documentation",
    "docs:watch": "compodoc packages -d docs -p tsconfig.json -s -o -w --includes tutorial --includesName Documentation",
    "watch:ui": "ng build ui --watch",
    "watch:data": "ng build data --watch",
    "watch:core": "ng build core --watch",
    "watch:calendar": "ng build calendar --watch",
    "postinstall": "cd website && npm i",
    "build:website": "cd website && npm run build",
    "docusaurus": "cd website && npm run start",
    "test:screenshots": "cd scripts && jest screenshot.test.js",
    "test:screenshots-update": "cd scripts && jest screenshot.test.js -u"
  },
  "private": true,
  "workspaces": [
    "packages/*/dist"
  ],
  "dependencies": {
    "@agm/core": "1.1.0",
    "@angular/animations": "^9.0.1",
    "@angular/cli": "^9.0.2",
    "@angular/common": "^9.0.1",
    "@angular/compiler": "^9.0.1",
    "@angular/core": "^9.0.1",
    "@angular/forms": "^9.0.1",
    "@angular/platform-browser": "^9.0.1",
    "@angular/platform-browser-dynamic": "^9.0.1",
    "@angular/router": "^9.0.1",
    "@ec.components/ace": "*",
    "@ec.components/calendar": "*",
    "@ec.components/core": "*",
    "@ec.components/data": "*",
    "@ec.components/location": "*",
    "@ec.components/medium-editor": "*",
    "@ec.components/style": "*",
    "@ec.components/tinymce": "*",
    "@ec.components/ui": "*",
    "ace-builds": "^1.4.8",
    "core-js": "^2.6.11",
    "ec.sdk": "0.21.16",
    "googlemaps": "^1.12.0",
    "lerna": "^3.20.2",
    "medium-editor": "^5.23.2",
    "moment-es6": "^1.0.0",
    "ngx-cookie": "^2.0.1",
    "ngx-drag-drop": "^2.0.0",
    "rxjs": "~6.5.4",
    "tinymce": "^4.9.8",
    "tslib": "^1.10.0",
    "x.ui": "10.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular-devkit/build-ng-packagr": "~0.900.2",
    "@angular/compiler-cli": "^9.0.1",
    "@angular/language-service": "^9.0.1",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.5.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "conventional-changelog": "^3.1.18",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.9.0",
    "jest-image-snapshot": "^2.12.0",
    "karma": "4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^9.0.0",
    "protractor": "^5.4.3",
    "puppeteer": "^1.20.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "3.7.5"
  },
  "browser": {
    "vm": false,
    "http": false,
    "https": false,
    "process": false,
    "global": false
  }
}
